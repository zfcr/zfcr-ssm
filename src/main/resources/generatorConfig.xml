		userId="root" password="admin">  
        </jdbcConnection>  
        <javaTypeResolver>  
            <property name="forceBigDecimals" value="false"/>  
        </javaTypeResolver>  
        <!-- 生成实体类的路径，com.project.ssm.entity 这个路径可以自动生成，但是必须有src这个路径-->
